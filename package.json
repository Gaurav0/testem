{
  "name": "testem",
  "description": "Test'em 'scripts! Javascript Unit testing made easy.",
  "keywords": [
    "javascript",
    "testing",
    "unittest",
    "browser"
  ],
  "version": "3.17.0",
  "author": "Toby Ho <airportyh@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/testem/testem.git"
  },
  "bugs": {
    "url": "http://github.com/testem/testem/issues"
  },
  "engines": {
    "node": ">= 7.*"
  },
  "license": "MIT",
  "dependencies": {
    "@xmldom/xmldom": "^0.8.11",
    "backbone": "^1.6.1",
    "bluebird": "^3.7.2",
    "charm": "^1.0.2",
    "commander": "^14.0.3",
    "compression": "^1.8.1",
    "consolidate": "^1.0.4",
    "execa": "^9.6.1",
    "express": "^4.10.7",
    "fireworm": "^0.7.2",
    "glob": "^11.0.3",
    "http-proxy": "^1.18.1",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.23",
    "mkdirp": "^3.0.1",
    "mustache": "^4.2.0",
    "node-notifier": "^10.0.1",
    "npmlog": "^6.0.0",
    "printf": "^0.6.1",
    "rimraf": "^6.1.3",
    "socket.io": "^4.8.3",
    "spawn-args": "^0.2.0",
    "styled_string": "0.0.1",
    "tap-parser": "^18.3.0",
    "tmp": "^0.2.5"
  },
  "files": [
    "lib",
    "public",
    "README.md",
    "testem.js",
    "assets",
    "package.json",
    "views"
  ],
  "scripts": {
    "install:all": "npm install && npm install phantomjs-prebuilt",
    "test": "mocha tests/*_tests.js tests/**/*_tests.js",
    "cover": "cover run ./node_modules/.bin/_mocha tests/*_tests.js tests/**/*_tests.js; cover report html; open cover_html/index.html",
    "integration": "node ./bin/run-integration.js",
    "browser-tests": "cd examples/saucelabs/ && ../../testem.js ci -d",
    "lint": "eslint ."
  },
  "devDependencies": {
    "bluebird-retry": "^0.11.0",
    "browserify": "^17.0.1",
    "chai": "^6.2.2",
    "chai-files": "^1.4.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "cheerio": "1.2.0",
    "dirty-chai": "^3.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-chai-expect": "^3.1.0",
    "eslint-plugin-mocha": "^11.2.0",
    "mocha": "^11.7.5",
    "request": "^2.88.2",
    "saucie": "^3.3.3",
    "shelljs": "^0.10.0",
    "sinon": "10.0.0",
    "sinon-chai": "^4.0.1",
    "socket.io-client": "^4.8.3",
    "tape": "^5.9.0"
  },
  "bin": {
    "testem": "./testem.js"
  },
  "main": "./lib/api.js"
}
